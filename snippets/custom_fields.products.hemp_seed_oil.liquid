{% comment %}
  DO NOT MODIFY THIS FILE DIRECTLY. THIS FILE WILL BE OVERWRITTEN.
  YOU CAN MODIFY THE LIQUID CODE FOR THIS CUSTOM FIELD HERE:
  https://customfields.bonify.io/dashboard/earthbar-nz.myshopify.com/products/configure/130063/edit
{% endcomment %}
{% if product.metafields.custom_fields["hemp_seed_oil"] != blank %}
  <div class="custom-field custom-field__hemp-seed-oil custom-field__type--checkbox">
    <h3 class="custom-field--title">Hemp Seed Oil</h3>
    <div class="custom-field--value">
      {% if product.metafields.custom_fields["hemp_seed_oil"] == 1 %}
        TRUE
      {% else %}
        FALSE
      {% endif %}
    </div>
  </div>
{% endif %}